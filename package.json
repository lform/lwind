{
	"name": "@lform/lwind",
	"version": "2.0.5",
	"description": "Customized tailwind framework",
	"main": "package.json",
	"private": false,
	"scripts": {
		"dev": "npx tailwindcss -i ./css/main.pcss -o ./dist/app.css",
		"watch": "npx tailwindcss -i ./css/main.pcss -o ./dist/app.css --watch",
		"prod": "NODE_ENV=production npx tailwindcss -i ./css/main.pcss -o ./dist/app.min.css --minify"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/lform/lwind.git"
	},
	"bugs": {
		"url": "https://github.com/lform/lwind/issues"
	},
	"homepage": "https://github.com/lform/lwind#readme",
	"keywords": [],
	"author": "Lform Design",
	"license": "ISC",
	"dependencies": {
		"@tailwindcss/forms": "^0.5.7",
		"autoprefixer": "^10.4.19",
		"postcss-advanced-variables": "^4.0.0",
		"postcss-atroot": "^0.2.4",
		"postcss-extend-rule": "^4.0.0",
		"postcss-import": "^16.1.0",
		"resolve-url-loader": "^5.0.0",
		"tailwindcss": "^3.4.3"
	}
}
